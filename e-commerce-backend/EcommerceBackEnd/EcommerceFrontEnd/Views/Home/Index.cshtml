                                                    </ul>
                                                </div>
                                            </div>
                                            <h4><a class="product_name" asp-action="singleproduct" asp-controller="products" asp-route-key="@item.ProductID.Protection()">@item.ProductName</a></h4>
                                            <div class="price-box">
                                                @if (item.DiscountValue != null && item.DiscountActive == true)
                                                {
                                                    var price = (item.DiscountValue.CalculateDiscountValue(item.Price, item.DiscountType)).FormartToDouble(item.Currency);

                                                    <span class="new-price new-price-2">@item.Currency@price</span>
                                                    <span class="old-price">@item.Currency@item.Price</span>
                                                    <span class="discount-percentage">-@item.DiscountValue@(item.DiscountType== "Percent" ? "%" : item.Currency)</span>
                                                }
                                                else {
                                                    <span class="new-price">@item.Currency@item.Price</span>
