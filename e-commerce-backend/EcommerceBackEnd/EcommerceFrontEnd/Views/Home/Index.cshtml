                                                    <span class="new-price">@item.Currency@item.Price</span>
                                                }
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            }
                        </div>
                    </div>
                </div>
            </div>
            <!-- Featured Product Area End Here -->
            <!-- Begin Featured Product Area -->
            <div class="col-lg-4">
                <div class="featured-product pt-sm-10 pt-xs-25">
                    <div class="li-section-title">
                        <h2>
                            <span>Special Products</span>
                        </h2>
                    </div>
                    <div class="featured-product-active-2 owl-carousel">
                        <div class="featured-product-bundle">
                            @foreach (var item in product3.ToList())
                            {
                                <div class="row">
                                    <div class="group-featured-pro-wrapper">
                                        <div class="product-img">
                                            <a asp-action="singleproduct" asp-controller="products" asp-route-key="@item.ProductID.Protection()">
                                                <img src="~/product_images/@item.Images.Split(",")[0]" alt="Li's Product Image">
                                            </a>
                                            @if (item.CreateDate.CheckArriralItem())
                                            {
                                                <span class="sticker">New</span>
                                            }
                                        </div>
                                        <div class="featured-pro-content">
                                            <div class="product-review">
                                                <h5 class="manufacturer">
                                                    <a href="product-details.html">Studio Design</a>
                                                </h5>
                                            </div>
                                            <div class="rating-box">
                                                <ul class="rating">
                                                    <li><i class="fa fa-star-o"></i></li>
                                                    <li><i class="fa fa-star-o"></i></li>
                                                    <li><i class="fa fa-star-o"></i></li>
                                                    <li class="no-star"><i class="fa fa-star-o"></i></li>
                                                    <li class="no-star"><i class="fa fa-star-o"></i></li>
                                                </ul>
                                            </div>
                                            <h4><a class="featured-product-name" asp-action="singleproduct" asp-controller="products" asp-route-key="@item.ProductID.Protection()">@item.ProductName</a></h4>
                                            <div class="price-box">
                                                @if (item.DiscountValue != null && item.DiscountActive == true)
                                                {
                                                    var price = (item.DiscountValue.CalculateDiscountValue(item.Price, item.DiscountType)).FormartToDouble(item.Currency);
                                                    <span class="new-price new-price-2">@item.Currency@price</span>                 
                                                }
                                                else
                                                {
                                                    <span class="new-price">@item.Currency@item.Price</span>
                                                }
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            }
                        </div>
                    </div>
                </div>
            </div>
            <!-- Featured Product Area End Here -->
        </div>
    </div>
</div>
<!-- Group Featured Product Area End Here -->